public class ForTest3 {
	public static void main(String[] args) {
//		for (int i = 0; i < 5; i++) {
//			for (int j = 0; j < 5; j++) {
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 5- i; j++) {
				System.out.print("*");
			}
			System.out.println(); // 라인 바꾸기
//				System.out.print("*");
		}
		System.out.println();
	}
}
