
public class ForTest2 {

	public static void main(String[] args) {
		int i =2;
		while(i <= 9) {
			int j = 1;
			while(j <= 9) {
				System.out.print(j+"*"+i+"="+i*j+" " );
				j++;
			}
			System.out.println(' ');
			i++;
		}
	
	}
}
